<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetAllianController</servlet-name>
  	<display-name>GetAllianController</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.web.GetAllianController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FileUploadController</servlet-name>
  	<display-name>FileUploadController</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.web.FileUploadController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetAllianController</servlet-name>
  	<url-pattern>/GetAllian</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FileUploadController</servlet-name>
  	<url-pattern>/fileupload</url-pattern>
  </servlet-mapping>
</web-app>
